:timeout: 30
:queues:
  - [often, 7]
  - [mailer, 7]
  - [default, 5]
  - [seldom, 3]
  - [order_images, 7]
  - [process_images, 7]
  - [rollbar, 5]
  - [deliver_order, 5]
  - [low, 5]


